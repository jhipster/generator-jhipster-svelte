{
	"name": "<%= dasherizedBaseName %>",
	"description": "Description for <%= baseName  %>",
	"version": "0.1.0",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"format": "prettier --write \"{,src/**/,cypress/**/}*.{md,json,js,svelte,css,html,yml}\"",
		"lint": "eslint .",
		"prepare": "husky install",
		"lint:fix": "npm run lint --fix",
		"cleanup": "rimraf <%= DIST_DIR %>",
		"start": "vite dev --port 9000 --open true",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest <%= CLIENT_MAIN_SRC_DIR %>",
		"test:watch": "npm run test -- --watch",
		"e2e": "npm run e2e:ci -- --headed",
		"e2e:ci": "cypress run --browser chrome",
        "cy:open": "cypress open --e2e --browser chrome",
		"build": "vite build",
		"webapp:build": "npm run build",
		"webapp:prod": "npm run build",
		"webapp:test": "npm run test"
	},
	"devDependencies": {
		"<%= blueprintjs.name %>": "<%= blueprintjs.version %>"
	},
	"type": "module"
}
